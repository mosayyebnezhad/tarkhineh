{
  "projects": [
    {
      "name": "backend",
      "directory": "back",
      "buildCommand": "npm install && npm run build",
      "outputDirectory": "dist",
      "framework": "nodejs"
    },
    {
      "name": "frontend",
      "directory": "front",
      "buildCommand": "npm install && npm run build",
      "outputDirectory": "build",
      "framework": "react"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Back/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/Front/$1"
    }
  ]
}